<%= simple_form_for @person, url: persons_path do |f| %>
  <%= f.input :name %>
  <%= f.input :cpf %>
  <%= f.input :cns %>
  <%= f.input :birthday %>
  <%= f.input :email %>
  <%#= f.input :data_nascimento, :as => :date_picker, :label => 'Data de Nascimento' %>
